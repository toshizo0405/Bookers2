<%= render  'books/list.html.erb', user: @user,book: @book_new %>


<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
     <thead>
       <tr>
         <th>image</th>
         <th>name</th>
         <th colspan="3"></th>
       </tr>
     </thead>
     <tbody>
      <% @users.each do |user| %>
         <tr>
            <td>
            ã€€
              <% if user.profile_image.attached? %>
               <%= image_tag user.profile_image, size: "80x80" %>
              <% else %>
                <%= image_tag 'default-image', size: "80x80" %>
              <% end %>
            </td>
            <td>
               <%= user.name %>
             </td>
             <td>
              <%= link_to "Show",user_path(user) %>
             </td>
          </tr>
      <% end %>
     </tbody>
  </table>